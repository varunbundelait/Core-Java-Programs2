package com.nit.core;

import java.util.Scanner;

public class SumSeriesInputByUser {
   public static void main(String[] args) {
	   int sum, n;
	   System.out.println("Enter a number");
	   Scanner sc=new Scanner(System.in);
	   n=sc.nextInt();
	  for(int i=1; i<=n; i++) {
		  int sum=sum + i;
		  System.out.println(+sum);
	  }
   }
}
